{
  "name": "o2oprotocol",
  "private": true,
  "version": "1.0.0",
  "description": "Smart Contracts (create and book listings) without directly interfacing with the Blockchain or IPFS.",
  "main": "index.js",
  "scripts": {
    "pretest": "truffle compile",
    "chain:private": "ganache-cli -p 8545 &",
    "test": "truffle test --network development",
    "test:watch": "watch \"reset && npm test\" test/ contracts/",
    "compile": "truffle compile --reset",
    "migrate:kovan": "truffle migrate --network kovan",
    "migrate:rinkeby": "truffle migrate --network rinkeby",
    "migrate:ropsten": "truffle migrate --network ropsten",
    "migrate:mainnet": "truffle migrate --network mainnet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://o2oprotocol@github.com/o2oprotocol/o2oprotocol.git"
  },
  "keywords": [
    "O2O Protocol",
    "Blockchain",
    "Smart Contract",
    "Ethereum",
    "IPFS"
  ],
  "files": [
    "build/"
  ],
  "author": "o2oprotocol.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/o2oprotocol/o2oprotocol/issues"
  },
  "homepage": "https://github.com/o2oprotocol/o2oprotocol#readme",
  "dependencies": {
    "dotenv": "^5.0.1",
    "ipfs-api": "^18.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "truffle": "^4.1.3",
    "truffle-hdwallet-provider": "^0.0.3",
    "uport-connect": "^0.7.1",
    "web3": "^1.0.0-beta.33"
  },
  "devDependencies": {
    "ganache-cli": "^6.1.0",
    "solidity-coverage": "^0.4.14",
    "solium": "^1.1.6"
  }
}
