{
  "name": "o2oprotocol",
  "version": "0.0.18",
  "description": "Smart Contracts (listing, purchase, controller) without directly interfacing with the Blockchain or IPFS.",
  "main": "./dist/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "prepare": "echo prepare",
    "build": "npm run build:contracts && npm run build:webpack",
    "build:watch": "npm run build:contracts && npm run build:webpack -- --watch",
    "build:contracts": "cd ../contracts && npx truffle compile",
    "build:webpack": "webpack --config webpack.config.js",
    "test:webpack": "webpack-dev-server --hot --config test/webpack.config.js",
    "test:mocha": "mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://o2oprotocol@github.com/o2oprotocol/o2oprotocol.git"
  },
  "keywords": [
    "O2O Protocol",
    "Blockchain",
    "Smart Contract",
    "Ethereum",
    "IPFS"
  ],
  "files": [
    "dist"
  ],
  "author": "o2oprotocol.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/o2oprotocol/o2oprotocol/issues"
  },
  "homepage": "https://github.com/o2oprotocol/o2oprotocol#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node7": "^1.5.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.0",
    "flow-bin": "^0.51.1",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.0.6",
    "mocha": "^3.5.0",
    "mocha-loader": "^1.1.3",
    "sinon": "^2.4.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "web3": "^0.20.1",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.11.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "bs58": "^4.0.1",
    "dotenv": "^2.0.0",
    "ipfs-api": "^14.3.7",
    "map-cache": "^0.2.2",
    "npm": "^5.8.0",
    "truffle-contract": "^3.0.4",
    "util.promisify": "^1.0.0"
  }
